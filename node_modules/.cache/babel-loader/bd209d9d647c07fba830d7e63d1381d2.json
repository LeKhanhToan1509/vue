{"ast":null,"code":"import SubHeader from \"@/components/SubHeader.vue\";\nimport ProductCard from \"@/components/home_components/cards/ProductCard.vue\";\nimport ProductPreloader from \"@/components/preloaders/ProductPreloader.vue\";\nimport MainHeader from \"@/components/MainHeader.vue\";\nimport MainFooter from \"@/components/MainFooter.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"ShopView\",\n  components: {\n    SubHeader,\n    ProductCard,\n    MainHeader,\n    MainFooter,\n    \"product-preloader\": ProductPreloader\n  },\n\n  data() {\n    return {\n      products: [],\n      page: 1,\n      loaded: false,\n      total: 0\n    };\n  },\n\n  created() {\n    this.getProducts();\n  },\n\n  methods: {\n    getProducts() {\n      this.loaded = false;\n      axios.get(`https://gorana.onrender.com/products?page=${this.page}`).then(res => {\n        this.products = res.data.results.map(product => {\n          product.images[0] = product.images[0].replace(\"http\", \"https\");\n          return product;\n        });\n        this.total = res.data.total_pages;\n        this.loaded = true;\n      }).catch(err => {\n        console.log(err);\n        this.loaded = false;\n      });\n    },\n\n    fetchNewPage(page) {\n      this.page = page;\n      this.getProducts();\n      window.scrollTo(0, 0);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyEA,OAAOA,SAAP,MAAsB,4BAAtB;AACA,OAAOC,WAAP,MAAwB,oDAAxB;AACA,OAAOC,gBAAP,MAA6B,8CAA7B;AAEA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,MAAI,EAAE,UADK;AAEXC,YAAU,EAAE;AACRP,aADQ;AAERC,eAFQ;AAGRE,cAHQ;AAIRC,cAJQ;AAKR,yBAAqBF;AALb,GAFD;;AASXM,MAAI,GAAG;AACH,WAAO;AACHC,cAAQ,EAAE,EADP;AAEHC,UAAI,EAAE,CAFH;AAGHC,YAAM,EAAE,KAHL;AAIHC,WAAK,EAAE;AAJJ,KAAP;AAMH,GAhBU;;AAiBXC,SAAO,GAAG;AACN,SAAKC,WAAL;AACH,GAnBU;;AAoBXC,SAAO,EAAE;AACLD,eAAW,GAAG;AACV,WAAKH,MAAL,GAAc,KAAd;AACAN,WAAI,CACCW,GADL,CACU,6CAA4C,KAAKN,IAAK,EADhE,EAEKO,IAFL,CAEWC,GAAD,IAAS;AACX,aAAKT,QAAL,GAAgBS,GAAG,CAACV,IAAJ,CAASW,OAAT,CAAiBC,GAAjB,CAAsBC,OAAD,IAAa;AAC9CA,iBAAO,CAACC,MAAR,CAAe,CAAf,IAAoBD,OAAO,CAACC,MAAR,CAAe,CAAf,EAAkBC,OAAlB,CAChB,MADgB,EAEhB,OAFgB,CAApB;AAIA,iBAAOF,OAAP;AACH,SANe,CAAhB;AAOA,aAAKT,KAAL,GAAaM,GAAG,CAACV,IAAJ,CAASgB,WAAtB;AACA,aAAKb,MAAL,GAAc,IAAd;AACH,OAZL,EAaKc,KAbL,CAaYC,GAAD,IAAS;AACZC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKf,MAAL,GAAc,KAAd;AACH,OAhBL;AAiBH,KApBI;;AAqBLkB,gBAAY,CAACnB,IAAD,EAAO;AACf,WAAKA,IAAL,GAAYA,IAAZ;AACA,WAAKI,WAAL;AACAgB,YAAM,CAACC,QAAP,CAAgB,CAAhB,EAAmB,CAAnB;AACH;;AAzBI;AApBE,CAAf","names":["SubHeader","ProductCard","ProductPreloader","MainHeader","MainFooter","axios","name","components","data","products","page","loaded","total","created","getProducts","methods","get","then","res","results","map","product","images","replace","total_pages","catch","err","console","log","fetchNewPage","window","scrollTo"],"sourceRoot":"","sources":["D:\\empty\\BuyIt\\src\\views\\User\\ShopView.vue"],"sourcesContent":["<template>\n    <main-header />\n    <sub-header\n        heading=\"#stayhome\"\n        subHeading=\"Save more with coupons up to 70% off\"\n    />\n    <div class=\"container\">\n        <div class=\"product__container\" v-if=\"loaded\">\n            <product-card\n                v-for=\"(product, index) in products\"\n                :key=\"index\"\n                :productId=\"product._id\"\n                :productName=\"product.name\"\n                :brand=\"product.brand\"\n                :price=\"product.price\"\n                :currency=\"product.currency\"\n                :ratings=\"product.rating\"\n                :image_url=\"product.images[0]\"\n                :in_stock=\"product.in_stock\"\n            />\n        </div>\n        <product-preloader v-else> Loading products... </product-preloader>\n    </div>\n    <div class=\"pagination\">\n        <vue-awesome-paginate\n            :total-items=\"total\"\n            :max-pages-shown=\"3\"\n            v-model=\"page\"\n            :show-breakpoint-buttons=\"false\"\n            :on-click=\"fetchNewPage\"\n            paginate-buttons-class=\"btn\"\n            active-page-class=\"btn-active\"\n            back-button-class=\"back-btn\"\n            next-button-class=\"next-btn\"\n            :hide-prev-next-when-ends=\"true\"\n        >\n            <template #prev-button>\n                <svg\n                    xmlns=\"http://www.w3.org/2000/svg\"\n                    width=\"16\"\n                    height=\"16\"\n                    fill=\"currentColor\"\n                    class=\"bi bi-arrow-left\"\n                    viewBox=\"0 0 16 16\"\n                >\n                    <path\n                        fill-rule=\"evenodd\"\n                        d=\"M15 8a.5.5 0 0 0-.5-.5H2.707l3.147-3.146a.5.5 0 1 0-.708-.708l-4 4a.5.5 0 0 0 0 .708l4 4a.5.5 0 0 0 .708-.708L2.707 8.5H14.5A.5.5 0 0 0 15 8z\"\n                    />\n                </svg>\n            </template>\n            <template #next-button>\n                <svg\n                    xmlns=\"http://www.w3.org/2000/svg\"\n                    width=\"18\"\n                    height=\"18\"\n                    fill=\"currentColor\"\n                    class=\"bi bi-arrow-right\"\n                    viewBox=\"0 0 16 16\"\n                >\n                    <path\n                        fill-rule=\"evenodd\"\n                        d=\"M1 8a.5.5 0 0 1 .5-.5h11.793l-3.147-3.146a.5.5 0 0 1 .708-.708l4 4a.5.5 0 0 1 0 .708l-4 4a.5.5 0 0 1-.708-.708L13.293 8.5H1.5A.5.5 0 0 1 1 8z\"\n                    />\n                </svg>\n            </template>\n        </vue-awesome-paginate>\n    </div>\n\n    <main-footer />\n</template>\n\n<script>\nimport SubHeader from \"@/components/SubHeader.vue\";\nimport ProductCard from \"@/components/home_components/cards/ProductCard.vue\";\nimport ProductPreloader from \"@/components/preloaders/ProductPreloader.vue\";\n\nimport MainHeader from \"@/components/MainHeader.vue\";\nimport MainFooter from \"@/components/MainFooter.vue\";\n\nimport axios from \"axios\";\n\nexport default {\n    name: \"ShopView\",\n    components: {\n        SubHeader,\n        ProductCard,\n        MainHeader,\n        MainFooter,\n        \"product-preloader\": ProductPreloader,\n    },\n    data() {\n        return {\n            products: [],\n            page: 1,\n            loaded: false,\n            total: 0,\n        };\n    },\n    created() {\n        this.getProducts();\n    },\n    methods: {\n        getProducts() {\n            this.loaded = false;\n            axios\n                .get(`https://gorana.onrender.com/products?page=${this.page}`)\n                .then((res) => {\n                    this.products = res.data.results.map((product) => {\n                        product.images[0] = product.images[0].replace(\n                            \"http\",\n                            \"https\"\n                        );\n                        return product;\n                    });\n                    this.total = res.data.total_pages;\n                    this.loaded = true;\n                })\n                .catch((err) => {\n                    console.log(err);\n                    this.loaded = false;\n                });\n        },\n        fetchNewPage(page) {\n            this.page = page;\n            this.getProducts();\n            window.scrollTo(0, 0);\n        },\n    },\n};\n</script>\n\n<style>\n.btn {\n    border: none;\n    background-color: #f2f2f2;\n    padding-inline: 20px;\n    height: 40px;\n    border: 2px solid var(--grey-2);\n    font-size: 1.7rem;\n    margin-inline: 5px;\n    cursor: pointer;\n}\n\n.back-btn,\n.next-btn {\n    background-color: #f2f2f2;\n    color: black;\n}\n\n.btn:hover {\n    background-color: #083e46;\n    border-color: #083e46;\n    color: white;\n}\n\n.btn-active {\n    background-color: #083e46;\n    color: white;\n    border-color: #083e46;\n}\n\n.pagination {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    margin: 2rem 0;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}